{
  "name": "trpc-msgpack",
  "version": "0.0.0",
  "description": "MessagePack encoder for tRPC WebSocket connections",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "trpc",
    "msgpack",
    "messagepack",
    "websocket",
    "encoder",
    "binary",
    "serialization"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sirajchokshi/trpc-msgpack.git"
  },
  "peerDependencies": {
    "@msgpack/msgpack": ">=3.0.0",
    "@trpc/client": ">=11.9.0",
    "@trpc/server": ">=11.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@msgpack/msgpack": "3.1.3",
    "@trpc/client": "11.9.0",
    "@trpc/server": "11.9.0",
    "tsup": "8.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
